<p id="notice"><%= notice %></p>

<p>
  <strong>Import name:</strong>
  <%= @import.import_name %>
</p>

<div id="records">
  <h5>Records</h5>
  <% @import.records.each do |record| %>
    <%= render partial: "records/record", locals: {record: record} %>
  <% end %>
</div>

<%= link_to 'Edit', edit_import_path(@import) %> |
<%= link_to 'Back', imports_path %>
